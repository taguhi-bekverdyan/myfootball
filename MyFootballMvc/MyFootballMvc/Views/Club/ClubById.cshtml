                    @for (int i = 0; i < 14; ++i)
                    {
                        <div class="person">
                            <div class="person-head">
                                <figure class="person-head-wrap">
                                    <img src="//premierleague-static-files.s3.amazonaws.com/premierleague/photos/players/250x250/o18757.png" alt="person-name" class="person-img">
